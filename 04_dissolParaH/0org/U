FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            pressureInletVelocity;
        phi             phi;
        value           uniform (0 0 0);
    }

    outlet
    {
        type            pressureInletVelocity;
        phi             phi;
        value           uniform (0 0 0);
    }

    insolubleWall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    top
    {
        type            empty;
    }
    bottom
    {
        type            empty;
    }

    solubleWall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}

// ************************************************************************* //
