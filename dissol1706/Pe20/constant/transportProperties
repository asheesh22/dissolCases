FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

//  Units: l0 unit of point field and <v> = Q/A  //

transportModel Newtonian;

Kinv         Kinv [ 0 -2 0 0 0 0 0 ] 0.0;        // Inverse permeability
nu             nu [ 0 2 -1 0 0 0 0 ] 2.5e+1;     // b^2/s (b is length unit)
D               D [ 0 2 -1 0 0 0 0 ] 2.5e-2;     // b^2/s
lR             lR [ 0 1  0 0 0 0 0 ] 1.0;        // Reactive length D/k

