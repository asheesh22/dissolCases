FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel Newtonian;

Kinv         Kinv [ 0 -2 0 0 0 0 0 ] 48.0;   // Inverse permeability = 12/h^2
nu             nu [ 0 2 -1 0 0 0 0 ] 60;     // <v> = 1
D               D [ 0 2 -1 0 0 0 0 ] 0.06;   // Pe =  1/D
l_T                                  0.2;    // Reactive length D/k


